{
  "manifest_version": 2,

  "name": "mangoext",
  "description": "A revolution in mango technology",
  "version": "0.2.1",

  "content_scripts": [{
    "matches": ["http://www.mangareader.net/*"],
    "css": ["main.css"],
    "js": ["main.js"],
    "run_at": "document_end"
  }],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}