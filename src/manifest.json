{
  "manifest_version": 2,

  "name": "mangoext",
  "description": "A revolution in mango technology",
  "version": "0.4.0",

  "content_scripts": [{
    "matches": ["http://www.mangareader.net/*"],
    "css": ["common.css", "mangoreader.css"],
    "js": ["common.js", "mangoreader.js"],
    "run_at": "document_end"
  }, {
    "matches": ["http://mangafox.me/*"],
    "css": ["common.css", "mangofox.css"],
    "js": ["common.js", "mangofox.js"],
    "run_at": "document_end"
  }, {
    "matches": ["*://bato.to/*"],
    "css": ["common.css", "batoto.css"],
    "js": ["common.js", "batoto.js"],
    "run_at": "document_end"
  }],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
